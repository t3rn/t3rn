{
  "name": "executor",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "ts-node src/index.ts | npx pino-pretty -c -S",
    "dev": "LOG_LEVEL=debug ts-node src/index.ts | npx pino-pretty -c -S",
    "export": "LOG_LEVEL=debug yarn start >> logger.log",
    "fmt": "npx prettier --no-semi --tab-width 4 --print-width 140 --write config/*.ts src/*.ts src/**/*.ts src/**/**/*.ts",
    "build-docs": "yarn fmt && npx typedoc"
  },
  "dependencies": {
    "@polkadot/api": "^9.8.2",
    "@t3rn/sdk": "../sdk",
    "@t3rn/types": "../types",
    "@types/node": "^18.11.9",
    "axios": "^1.1.2",
    "bn.js": "^5.2.1",
    "debug": "4.3.4",
    "dotenv": "16.0.0",
    "pino": "^8.7.0",
    "pino-pretty": "^9.1.1",
    "rxjs": "^7.5.7",
    "ts-node": "10.7.0",
    "typedoc": "^0.23.21",
    "typescript": "^4.8.4"
  }
}
