{
  "name": "executor",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "EXECUTOR=example ts-node src/main.ts",
    "dev": "LOG_LEVEL=debug EXECUTOR=example ts-node src/main.ts | npx pino-pretty -c -S",
    "lint": "eslint --ext .ts src",
    "lint:report": "eslint src --ext .ts --output-file eslint_report.json --format json",
    "export": "LOG_LEVEL=debug pnpm run start >> logger.log",
    "fmt": "prettier --write src config tests",
    "build-docs": "npx typedoc && cp -R docs ../../../docs/executor",
    "test": "mocha -r ts-node/register 'tests/**/*.ts'",
    "test-single": "mocha -r ts-node/register"
  },
  "exclude": [
    "./tests/"
  ],
  "dependencies": {
    "@polkadot/api": "^10.9.1",
    "@polkadot/keyring": "^12.2.2",
    "@polkadot/types": "^10.9.1",
    "@polkadot/util": "^12.2.2",
    "@polkadot/util-crypto": "^12.2.2",
    "@t3rn/sdk": "../sdk",
    "@t3rn/types": "../types",
    "@types/chai": "^4.3.4",
    "@types/chai-as-promised": "^7.1.5",
    "@types/debug": "^4.1.8",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.2.6",
    "axios": "^1.4.0",
    "bn.js": "^5.2.1",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "debug": "4.3.4",
    "dotenv": "16.3.1",
    "mocha": "^10.2.0",
    "node-gyp": "^9.3.1",
    "pino": "^8.14.1",
    "pino-pretty": "^10.0.0",
    "prettier": "^2.8.8",
    "prom-client": "^14.2.0",
    "rxjs": "^7.5.7",
    "sinon": "^15.2.0",
    "standard": "^17.0.0",
    "ts-mockito": "^2.6.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.7",
    "types": "link:@polkadot/keyring/types"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint": "^8.43.0",
    "mocha-chai-jest-snapshot": "^1.1.4",
    "typescript": "4.9.4"
  }
}
