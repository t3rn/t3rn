{
  "circuitPortal": {
    "readLatestGatewayHeight": {
      "description": "Returns the gateways height knows by circuit",
      "params": [
        {
          "name": "chain_id",
          "type": "[u8; 4]"
        },
        {
          "name": "at",
          "type": "Hash",
          "isOptional": true
        }
      ],
      "type": "ReadLatestGatewayHeight"
    }
  },
  "xdns": {
    "fetchRecords": {
      "description": "Fetches all available XDNS Records on Circuit",
      "params": [
        {
          "name": "at",
          "type": "Hash",
          "isOptional": true
        }
      ],
      "type": "FetchXdnsRecordsResponse"
    },
    "fetchAbi": {
      "description": "Fetches abi of a specific gateway",
      "params": [
        {
          "name": "chain_id",
          "type": "ChainId"
        },
        {
          "name": "at",
          "type": "Hash",
          "isOptional": true
        }
      ],
      "type": "GatewayABIConfig"
    }
  },
  "portal": {
    "getLatestFinalizedHeader": {
      "description": "Fetches the latest finalized header of a gateway",
      "params": [
         {
          "name": "gateway_id",
          "type": "ChainId"
        }
      ],
      "type": "Result<Vec<u8>>"
    }
  }
}